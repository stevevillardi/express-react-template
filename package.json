    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.17.0",
    "googleapis": "^47.0.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "node-ews": "^3.4.2",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "readline": "^1.3.0"
  }
